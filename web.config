<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />

<handlers>
	<add name="iisnode" path="app.js" verb="*" modules="iisnode" />
</handlers>
    
  <customErrors mode="Off" />
	    
<rewrite>
	<rules>
		<rule name="sendToNode">
			<match url="/*" />
			<action type="Rewrite" url="app.js" />
		</rule>	
	</rules>
</rewrite>		    

    </system.webServer>
</configuration>
